
@model rtaplamaciBlog.Models.AnasayfaAyar
@{
    ViewBag.Description = Model.MDescripton;
    ViewBag.Author = Model.MAuthor;
    ViewBag.Url = Model.MUrl;
    ViewBag.Keywords = Model.MKeywords;
    ViewBag.Title = Model.STitle;
    Layout = "~/Views/_Shared/_SiteLayout.cshtml";
}
<section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
    <div class="my-auto">
        <div>
            <img src="~/Images/General/@Model.GDizin" class="rounded" style="width: 100px;" alt="@Model.GAlt">
        </div>
        <h1 class="mb-0">
            @Model.BBaslik
            <span class="text-primary">@Model.IBaslik</span>
        </h1>
        <div class="subheading mb-5">
        </div>
        <p class="mb-5">
            <h3>@Model.SBaslik</h3>
            @Html.Raw(Model.Icerik)
    </div>
</section>